    "bignumber.js": "^9.0.1",
    "boc-contract-core": "2.0.0-beta-3",
    "eslint-plugin-mocha": "^9.0.0",
    "hardhat-deal": "^1.3.0",
    "ts-node": "^10.9.1"
  },
  "scripts": {
    "chain": "hardhat node --hostname 0.0.0.0 --no-deploy",
    "chain-deploy": "hardhat node --hostname 0.0.0.0",
    "coverage": "hardhat coverage",
    "docgen": "hardhat docgen",
    "topup": "hardhat run scripts/top-up.js --network localhost",
    "test_adapter_add_ethi": "hardhat run scripts/test-adapter-add-ethi.js --network localhost",
    "test_adapter_remove_ethi": "hardhat run scripts/test-adapter-remove-ethi.js --network localhost",
    "test_adapter_add_usdi": "hardhat run scripts/test-adapter-add-usdi.js --network localhost",
    "test_adapter_remove_usdi": "hardhat run scripts/test-adapter-remove-usdi.js --network localhost",
    "update-aggregator": "hardhat run scripts/update-aggregator.js --network localhost",
    "deploy-usd": "hardhat deploy --tags usd_modules --network localhost ",
    "deploy-eth": "hardhat deploy --tags eth_modules --network localhost ",
    "deploy-lending": "hardhat deploy --tags lending_modules --network localhost ",
    "deploy": "yarn topup && hardhat run scripts/deploy-test.js --network localhost ",
    "deploy-all": "yarn topup && hardhat deploy --tags full_modules --network localhost && yarn sync-apollo",
    "sync-apollo": "hardhat run scripts/sync-apollo.ts --network localhost",
    "chainlink-add-steth": "hardhat run scripts/chainlink-add-steth.js --network localhost",
    "grant": "hardhat run scripts/grant.js --network localhost",
    "lend": "hardhat run scripts/simple-lend.js --network localhost",
    "uniswap-v3-swap": "hardhat run scripts/uniswap-v3-swap.js --network localhost",
    "uniswap-v3-increase-observation": "hardhat run scripts/uniswap-v3-increase-observation.js --network localhost",
    "lint:sol": "solhint --config ./.solhint.json --max-warnings 0 \"contracts/**/*.sol\"",
    "release": "hardhat run scripts/deploy.js --network mainnet",
    "upgrade:mainnet": "hardhat run scripts/upgrade.js --network mainnet",
    "grow": "hardhat run scripts/mainnet-grow.js --network localhost",
    "eslint": "eslint ./test",
    "test": "hardhat compile",
    "mint": "hardhat run scripts/simple-mint.js --network localhost"
  }
}
